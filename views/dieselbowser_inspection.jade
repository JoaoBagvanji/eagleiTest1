				li.inspdiariaIcon(style="user-select:none") <span class="hide-on-small-only">Folgas no Cubo</span> <span class="tiny"> <img class="hide-on-med-and-up" style="width:25px" src="/img/folga.png">
			fieldset
				h2(class="fs-title selmecAzul") <span data-i18n="demo.inspdiaria_form.dados_gerais"></span>
					.row#verificar(data = "#{v1}")
						.row
							.col.s12.m6
								.input-field(oninput="this.className = ''")
									i.material-icons.prefix.selmecAzul person
									label(for="motorista") <span data-i18n="demo.inspdiaria_form.motorista"></span>
									input(id="motorista", autocomplete="off",readonly, type="text", name="motorista", value="#{Bowser? Bowser[0].responsavel:DataU.nome}")
							.col.s12.m6
								label <span data-i18n="demo.inspdiaria_form.matricula"></span>
								if(!Bowser)
									select.browser-default(name="matriculadb", id="matriculadb")
										option(value="" disabled selected) Escolha a opção
										-for(var i=0; i<bowsers.length; i++)
											option(value="#{bowsers[i].matricula}", data-cont="#{bowsers[i]._id}") #{bowsers[i].matricula}
								else
									select.browser-default(name="matriculadb", id="matriculadb")
										option(value="#{Bowser[0].matricula}", data-cont="#{Bowser[0]._id}") #{Bowser[0].matricula}
						.row
							.input-field.col.s12.m6
								if(!Bowser)
									i.material-icons.prefix.selmecAzul rv_hookup
									label(for="dieselbowser") Diesel Bowser
									input.preencher(type="text", id="dieselbowser",  name="dieselbowser", value="#{veiculos ? veiculos[0].kilometragem: 0}")
								else
									i.material-icons.prefix.selmecAzul rv_hookup
									label(for="dieselbowser") Diesel Bowser
									input.preencher(type="text", id="dieselbowser",  name="dieselbowser", value="#{Bowser ? Bowser[0].dieselbowser: 0}")
							.input-field.col.s12.m6
								i.material-icons.prefix.selmecAzul date_range
								if(!Bowser)
									label(for="datta") <span data-i18n="demo.inspdiaria_form.data_hora"></span>
									input.preencher(type="text", id="datta", readonly,name="data", value="#{veiculos? veiculos[0].datta : ((new Date()).getDate()<10? '0'+(new Date()).getDate():(new Date()).getDate())+'/'+(((new Date()).getMonth()+1)<10? ('0'+((new Date()).getMonth()+1)):((new Date()).getMonth()+1))+'/'+((new Date()).getFullYear())+'   '+((new Date()).getHours()<10? ('0'+(new Date()).getHours()): (new Date()).getHours() )+' : '+((new Date()).getMinutes()<10? ('0'+(new Date()).getMinutes()):(new Date()).getMinutes())}")
